<?xml version="1.0" encoding="utf-8"?>

<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee;http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">

    
 
	
	<servlet>
	    <servlet-name>Export</servlet-name>
	    <servlet-class>com.appspot.Accent.controller.ServletExport</servlet-class>	    
	</servlet>
	<servlet-mapping>
	    <servlet-name>Export</servlet-name>
	    <url-pattern>/Userlijst.zip</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>ImportServlet</servlet-name>
		<servlet-class>com.appspot.Accent.controller.ServletImportLeerlingen</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ImportServlet</servlet-name>
		<url-pattern>/Importeer.do</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>ImportStages</servlet-name>
		<servlet-class>com.appspot.Accent.controller.ServletImportStages</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ImportStages</servlet-name>
		<url-pattern>/Importeers.do</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>StageAanmakenServlet</servlet-name>
		<servlet-class>com.appspot.Accent.controller.StageMakenServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>StageAanmakenServlet</servlet-name>
		<url-pattern>/StageAanmaken.do</url-pattern>
	</servlet-mapping>
	
